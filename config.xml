<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.coolappz.fixea" version="0.0.1">
    <name>fixea</name>
    <description>
        Fixea una aplicacion para aunar y potenciar la comunidad alrededor de las bicis fixie
    </description>
    <author email="larraz@gmail.com" href="http://google.es">
        Miguel Larraz Sala
    </author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <access origin="https://fixea-techanswering.rhcloud.com/"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
<platform name="android">    
    <icon src="icon.png" density="mdpi" />
    <icon src="icon.png" density="ldpi" />
    <icon src="icon.png" density="hdpi" />
    <icon src="icon.png" density="xdpi" />
    <icon src="icon.png" density="xxdpi" />
</platform>

</widget>
